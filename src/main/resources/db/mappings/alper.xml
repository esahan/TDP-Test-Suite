<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm
	http://java.sun.com/xml/ns/persistence/orm_2_0.xsd" version="2.0">

	<package>com.ykb.tdp.testsuite.model</package>

	<entity class="User">
		<sequence-generator name="UserSeqGenerator" allocation-size="1" initial-value="1" sequence-name="SEQ_TDP_TESTSUITE_USERS" />
		<attributes>
			<id name="id">
				<column name="ID" />
				<generated-value strategy="SEQUENCE" generator="UserSeqGenerator" />
			</id>
		</attributes>
	</entity>
</entity-mappings>